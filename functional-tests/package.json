{
  "name": "opensearch-index-manager-functional-tests",
  "version": "1.0.0",
  "description": "Functional tests for OpenSearch Index Manager plugin using Playwright",
  "scripts": {
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:chromium": "playwright test --project=chromium",
    "test:firefox": "playwright test --project=firefox",
    "test:webkit": "playwright test --project=webkit",
    "test:report": "playwright show-report",
    "install:browsers": "playwright install",
    "install:deps": "playwright install-deps",
    "lint": "tsc --noEmit",
    "cleanup": "node -e \"require('fs').rmSync('../test-output/functional-tests', { recursive: true, force: true })\" 2>/dev/null || true"
  },
  "keywords": [
    "opensearch",
    "dashboards",
    "playwright",
    "e2e",
    "functional-testing"
  ],
  "author": "",
  "license": "Apache-2.0",
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.0"
  },
  "dependencies": {
    "node-fetch": "^3.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}